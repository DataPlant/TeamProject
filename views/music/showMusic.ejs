            <a class='dropdown-item' href='/music'id='back'> Go Back </a>
            <a class='dropdown-item' href="/music/<%= showMusic._id %>/edit" id='edit'>Edit mushroom</a>
            <form action='/music/<%= showMusic._id %>?_method=DELETE' method='POST' class='dropdown-item'id='deleteform'>
