<body>
    <%- include('../partials/navbar.ejs') %>
    <h1 id='header'>Add a TV Show</h1>
    <div id='body'>
        <img id='img' src='../images/tv.png'>
        <br>
        <br>
        <form method='POST' action='/tv-show/newTV'>
            <label for='title'>Title of TV Show:</label>
            <input type='text' class='form-control'name='title'>
            <br>
            <label for="rating">Rating:</label>
            <input type="text" class='form-control' name="rating">
            <br>
            <label for="genre">Genre:</label>
            <input type="text" class='form-control' name="genre">
            <br>
            <label for="summary">Summary:</label>
            <input type="text" class='form-control' name="summary">
            <br>
            <label for="tvcover" id='tvcover'>Cover:</label>
            <input type="text" class='form-control' name="tvcover">
            <br>
            <a href='/tv-show' id='back'> <button type='button' class='btn btn-default'>Go Back </a> 
            <button type='submit' class='btn btn-success' class='img-circle'>Submit </button>
        </form>
    </div>
</body>
